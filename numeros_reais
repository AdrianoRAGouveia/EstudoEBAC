programa {
  funcao inicio() {
      real num1, num2, teste1, teste2

      escreva ("Digite dois valores:\n")
      leia (num1, num2)

      teste1= num1 + num2
      teste2= num1 / num2

      escreva ("O valor de teste1 é: ",teste1, "\n")
      escreva ("O valor de Teste2 é: ",teste2, "\n")
  }
}
